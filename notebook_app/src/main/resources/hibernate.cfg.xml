<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 5.3//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration">
<hibernate-configuration>
    <session-factory>
        <property name="connection.url">jdbc:sqlite:feladat.db</property>
        <property name="hibernate.dialect">org.sqlite.hibernate.dialect.SQLiteDialect</property>
        <mapping class="com.notebook.Models.OS"/>
        <mapping class="com.notebook.Models.Notebook"/>
        <mapping class="com.notebook.Models.CPU"/>
    </session-factory>
</hibernate-configuration>
