<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.notebook.NotebookController">
    <children>
        <BorderPane prefHeight="506.0" prefWidth="760.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <HBox prefHeight="60.0" prefWidth="421.0" style="-fx-background-color: #4057e9;" BorderPane.alignment="CENTER">
                    <children>
                        <Label contentDisplay="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="60.0" prefWidth="750.0" text="JavaFX beadandó" textAlignment="CENTER" textFill="WHITE">
                            <padding>
                                <Insets left="275.0" />
                            </padding>
                            <font>
                                <Font size="22.0" />
                            </font>
                     <HBox.margin>
                        <Insets left="275.0" />
                     </HBox.margin>
                        </Label>
                    </children>
                </HBox>
            </top>
            <center>
                <HBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="1300.0" spacing="20.0" BorderPane.alignment="TOP_CENTER">
                    <children>
                        <Button fx:id="databaseButton" mnemonicParsing="false" onAction="#loadDatabase" prefHeight="40.0" prefWidth="212.0"  text="Adatbázis" textFill="BLACK">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Button>
                        <Button fx:id="soapClientButton" mnemonicParsing="false" onAction="#loadSoapClient" prefHeight="40.0" prefWidth="212.0"  text="Soap Kliens" textFill="BLACK">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Button>
                        <Button fx:id="parhuzamosButton" mnemonicParsing="false" onAction="#loadParhuzamos" prefHeight="40.0" prefWidth="212.0"  text="Párhuzamos" textFill="BLACK">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Button>
                        <Button fx:id="forexButton" mnemonicParsing="false" onAction="#loadForex" prefHeight="40.0" prefWidth="212.0"  text="Forex" textFill="BLACK">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Button>
                    </children>
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
                </HBox>
            </center>
            <bottom>
                <AnchorPane fx:id="view" prefHeight="630.0" prefWidth="1300.0" BorderPane.alignment="CENTER" />
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
